<section class="two-col-list-section">
    <div class="container">
        <div class="two-col-list-title">
            <h2>{{ title|e('wp_kses_post') }}</h2>
        </div>
        <div class="two-col-list-row">
            {% for item in items %}
                <div class="two-col-list-col">
                    <div class="two-col-list-inner">
                        <div class="two-col-list-icon" style="background-image: url('{{ item.image }}');"></div>
                        <div class="two-col-list-content">{{ item.content|e('wp_kses_post') }}</div>
                    </div>
                </div>
            {% else %}
                Nothing have been found.
            {% endfor %}
        </div>
    </div>
</section>